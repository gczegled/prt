<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.inf.unideb.library.controllers.MainController">
   <children>
      <Text layoutX="229.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Bejelentkezés">
