      </Text>
      <TextField fx:id="userName" alignment="CENTER" focusTraversable="false" layoutX="200.0" layoutY="120.0" minHeight="30.0" minWidth="200.0" promptText="Oracle felhasználói név" />
      <PasswordField fx:id="password" alignment="CENTER" focusTraversable="false" layoutX="200.0" layoutY="170.0" minHeight="30.0" minWidth="200.0" promptText="Oracle jelszó" />
      <Button focusTraversable="false" layoutX="225.0" layoutY="230.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" onMouseClicked="#login" text="Belépés">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
